{
  "name": "openvoice",
  "version": "1.0.0",
  "description": "OpenVoice - AI-Powered Audio Enhancement",
  "private": true,
  "scripts": {
    "test": "npm run test:frontend && npm run test:backend",
    "test:frontend": "cd frontend && npm test -- --run",
    "test:backend": "cd backend && pytest tests/test_file_handler.py tests/test_audio_processor.py::TestAudioProcessor::test_initialization tests/test_audio_processor.py::TestAudioProcessor::test_load_audio tests/test_audio_processor.py::TestAudioProcessor::test_load_audio_mono_conversion tests/test_audio_processor.py::TestAudioProcessor::test_load_audio_resampling tests/test_audio_processor.py::TestAudioProcessor::test_save_audio tests/test_audio_processor.py::TestAudioProcessor::test_attenuation_and_gain_parameters tests/test_audio_processor.py::TestAudioProcessor::test_slider_to_db_mapping tests/test_audio_processor.py::TestAudioProcessor::test_gain_compensation_logic -v",
    "test:watch": "cd frontend && npm test",
    "test:coverage": "npm run test:coverage:frontend && npm run test:coverage:backend",
    "test:coverage:frontend": "cd frontend && npm run test:coverage",
    "test:coverage:backend": "cd backend && pytest tests/test_file_handler.py --cov=utils --cov-report=html --cov-report=term-missing",
    "install:all": "npm run install:frontend && npm run install:backend",
    "install:frontend": "cd frontend && npm install",
    "install:backend": "cd backend && pip install -r requirements.txt -r requirements-test.txt",
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend && uvicorn main:app --reload",
    "build": "cd frontend && npm run build",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:test": "docker-compose exec backend pytest -v"
  },
  "keywords": [
    "audio",
    "enhancement",
    "noise-reduction",
    "ai",
    "deepfilternet"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
